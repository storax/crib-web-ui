{
  "name": "crib",
  "version": "0.1.0",
  "description": "Web UI for crib",
  "main": "index.ts",
  "scripts": {
    "dev": "parcel serve src/index.html -d dist --public-url /dist/ --open",
    "build": "cross-env NODE_ENV=production parcel build src/index.html -d build --public-url .",
    "clean": "rimraf .cache/ && rimraf dist/ && rimraf build/",
    "test:unit": "jest --no-cache",
    "test:coverage": "jest --no-cache --coverage",
    "predeploy": "npm run build",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "author": "David Zuber",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.6.0",
    "autoprefixer": "^9.6.1",
    "cross-env": "^5.2.1",
    "jest": "^24.9.0",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.7.1",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.3",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.1.0",
    "axios": "^0.21.1",
    "jquery": "^3.5.0",
    "leaflet": "^1.5.1",
    "leaflet-draw": "^1.0.4",
    "slick": "^1.12.2",
    "swiper": "^4.5.1",
    "tinygradient": "^1.0.0",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.10",
    "vue-class-component": "^6.3.2",
    "vue-hot-reload-api": "^2.3.4",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "^3.1.3",
    "vue-slick": "^1.1.15",
    "vue2-leaflet": "^1.2.3",
    "vuetify": "^1.5.18",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "mapCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:storax/crib-web-ui.git"
  },
  "bugs": {
    "url": "https://github.com/storax/crib-web-ui/issues"
  },
  "homepage": "https://github.com/storax/crib-web-ui#readme"
}
